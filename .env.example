# データベース設定
DATABASE_URL=postgresql://user:password@postgres:5432/localdb?schema=public

# MinIO設定（ローカル開発環境用）
MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
